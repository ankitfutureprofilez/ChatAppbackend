{
    "builds": [
        {
            "src": "./index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/"
        }
    ],
    "functions": {
        "api/signup": {
            "runtime": "nodejs14.x",
            "memory": 512,
            "timeout": 30
        }
    }
}